<?xml version="1.0" encoding="utf-8" ?>
<widget id="SetAttribute.widget.SetAttribute" needsEntityContext="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>SetAttribute</name>
    <description>The description of this widget.</description>

    <icon>
        
    </icon>

    <properties>
        <property key="domQuery" type="string">
            <caption>Dom Query</caption>
            <category>General</category>
            <description>The query to the nodes the attribute has to be set on</description>
        </property>
         <property key="local" type="boolean" required="true" defaultValue="false">
             <caption>Restrict to Siblings</caption>
             <category>General</category>
             <description>Restrict query to only siblings of this widget.</description>
         </property>
         <property key="searchParents" type="boolean" required="true" defaultValue="false">
             <caption>Restrict to Parents</caption>
             <category>General</category>
             <description>Restrict query to only parents of this widget (can be used in combination with Siblings).</description>
         </property>
        <property key="attributes" type="object" isList="true" required="true">
            <caption>Attributes</caption>
            <category>General</category>
            <description>The message visible within the widget.</description>
            <properties>
                <property key="attribute" type="string" required="true">
                    <caption>Attribute</caption>
                    <category>Attributes</category>
                    <description></description>
                </property>
                <property key="value" type="string" required="false">
                  <caption>Value</caption>
                  <category>Attributes</category>
                  <description></description>
                </property>
                <property key="useDynamicValue" type="boolean" required="true" defaultValue="false">
                     <caption>Use Dynamic Value?</caption>
                     <category>Attributes</category>
                     <description>Use an attribute of the context object as the value?</description>
                 </property>
                <property key="dynamicValue" type="attribute" required="false" isPath="optional" pathType="reference">
                    <caption>Dynamic Value</caption>
                    <category>Attributes</category>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="String" />
                        <attributeType name="Enum" />
                    </attributeTypes>
                </property>
                
                <property key="append" type="boolean" defaultValue="false">
                    <caption>Append</caption>
                    <category>Attributes</category>
                    <description>Append the value to the attribute instead of overwriting it.</description>
                </property>
            </properties>
        </property>

    </properties>
</widget>
